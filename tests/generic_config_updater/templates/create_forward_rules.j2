[
    {
        "op": "add",
        "path": "/ACL_RULE",
        "value": {
            "{{ v4_table_name }}|RULE_1": {
                "DST_IP": "{{ ipv4_subnet }}",
                "PRIORITY": "9999",
                "PACKET_ACTION": "FORWARD"
                {% if v4_table_name != v6_table_name %}
                ,
                "ETHER_TYPE": "0x0800"
                {% endif %}
            },
            "{{v6_table_name }}|RULE_2": {
                "DST_IPV6": "{{ ipv6_subnet }}",
                "PRIORITY": "9998",
                "PACKET_ACTION": "FORWARD"
                {% if v4_table_name != v6_table_name %}
                ,
                "IP_TYPE": "IPV6ANY"
                {% endif %}
            }
        }
    }
]
